{
    "name": "next-sentence/sitemap-bundle",
    "type": "symfony-bundle",
    "description": "Sitemap Bundle for Symfony",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "symfony/framework-bundle": "^4.2|^5.0",
        "knplabs/knp-gaufrette-bundle": "^0.5",
        "symfony/templating": "^4.2|^5.0",
        "symfony/twig-bundle": "^4.2|^5.0",
        "symfony/yaml": "*",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.12"
    },
    "require-dev": {
        "lchrusciel/api-test-case": "^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.0",
        "phpspec/phpspec": "^6.0",
        "phpstan/phpstan-doctrine": "^0.11",
        "phpstan/phpstan-shim": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpstan/phpstan-webmozart-assert": "^0.11",
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master",
        "sensiolabs/security-checker": "^6.0",
        "sylius-labs/coding-standard": "^3.0",
        "symfony/debug-bundle": "^3.4|^4.1",
        "symfony/dotenv": "^4.2",
        "symfony/intl": "^3.4|^4.1",
        "symfony/web-profiler-bundle": "^3.4|^4.1",
        "symfony/web-server-bundle": "^3.4|^4.1"
    },
    "autoload": {
        "psr-4": {
            "LWC\\SitemapBundle\\": "src/"
        }
    },
    "conflict": {
        "symfony/symfony": "4.1.8",
        "symfony/browser-kit": "4.1.8",
        "symfony/dependency-injection": "4.1.8",
        "symfony/dom-crawler": "4.1.8",
        "symfony/routing": "4.1.8"
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload-dev": {
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts": {
        "security": "vendor/bin/security-checker security:check"
    }
}
